  SELECT * FROM RandomizedPCA (
    ON rpca_input AS InputTable
    USING
    RowIdColumn ('pid')
    ColumnIdColumn ('attribute')
    ValueColumn ('value_col') 
  ) AS dt
) WITH DATA;
</tt>
