SELECT * FROM Scale (
  ON scale_housing AS InputTable PARTITION BY ANY
  ON tmpScale AS statistic DIMENSION
  USING
  ScaleMethod ('midrange','mean','maxabs','range')
  Accumulate ('id')
) AS dt ORDER BY id,price,lotsize;</tt>
